<h1>Editing response_category</h1>

<% form_for(@response_category) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @response_category %> |
<%= link_to 'Back', response_categories_path %>