<h1>Listing response_categories</h1>

<table>
  <tr>
    <th>Name</th>
  </tr>

<% @response_categories.each do |response_category| %>
  <tr>
    <td><%=h response_category.name %></td>
    <td><%= link_to 'Show', response_category %></td>
    <td><%= link_to 'Edit', edit_response_category_path(response_category) %></td>
    <td><%= link_to 'Destroy', response_category, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New response_category', new_response_category_path %>